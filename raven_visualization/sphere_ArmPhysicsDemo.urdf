<?xml version="1.0"?>



<robot name="sphere_Demo">

  <link name="sphere">
    <visual>
      <geometry>
	<sphere radius="0.2" />
      </geometry>
    </visual>

    <inertial>
      <origin xyz = "0 0 0" rpy="0 0 0"/>
      <mass value = "1"/>
      <inertia
          ixx = "1"
          ixy = "0"
          ixz = "0"
          iyy = "1"
          iyz = "0"
          izz = "1"
          />
    </inertial>

    <collision>
      <origin xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.2" />
      </geometry>
    </collision>

  </link>
</robot>
